fun main() {
    println("In this code, prime numbers 1 up to 200 appears")

    for (num in 2..200) {
        var isPrime = true

        for (a in 2 until number) {
            if (number % a == 0) {
                isPrime = false
                break
            }
        }

        if (isPrime) {         
            println(number)
        }
    }
}
